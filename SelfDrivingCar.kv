#:kivy 2.0.0

<Form>:
    Map:
        center: self.parent.center

<Map>
    Car:
        center: self.parent.center

<Car>:
    size: 20, 10
    canvas:
        Rectangle:
            pos: self.pos
            size: self.size

<Ball>:
    size: 10,10
    canvas:
        Color:
            rgba: 1,0,0,1
        Ellipse:
            pos: self.pos
            size: self.size





